- var loggedIn = user_info
.container-fluid#header_background
  .col-md-12
    nav.navbar.navbar-default
      // Brand and toggle get grouped for better mobile display
      .navbar-header
        button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-1', aria-expanded='false')
          span.sr-only Toggle navigation
          span.icon-bar
          span.icon-bar
          span.icon-bar
        a.navbar-brand(href='/') ProjectArena
      // Collect the nav links, forms, and other content for toggling
      #bs-example-navbar-collapse-1.collapse.navbar-collapse

        ul.nav.navbar-nav.navbar-right
          li.login
            a(href='/').btn Home
          li.users
            a(href='/users').btn Users
          li.search
            a(href='/').btn Search
          li.profile
            a(href='/profile').btn Profile
          if loggedIn 
            li.logout
              a(href='/logout').btn Logout
          else
            li.login
              a(href='/register').btn Sign Up
            li.login
              a(href='/login').btn Login